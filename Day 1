#include <iostream>
#include <string>
#include<cstring>
#include <algorithm>

int GetFirstDigit(std::string line)
{
    int FirstDigit;
    std::string::iterator it; 
    for (it = line.begin(); it != line.end(); it++) { 
        if (std::isdigit(*it))
        {
            FirstDigit = *it - '0';
            break;
        }
    } 
    return(FirstDigit);
}

int GetLastDigit(std::string Line)
{
    int LastDigit;
    std::reverse(Line.begin(), Line.end());
    LastDigit = GetFirstDigit(Line);
    return(LastDigit);
}

int GetCalibrationvalue(std::string Line)
{
    int CalibrationValue;
    int FirstDigit;
    int LastDigit;
    FirstDigit = GetFirstDigit(Line);
    LastDigit = GetLastDigit(Line);
    return(10*FirstDigit + LastDigit);
}


int main()
{
    std::string Line;
    while(std::getline(std::cin, Line))
    {
        int CalibrationValue;
        CalibrationValue = GetCalibrationvalue(Line);
        std::cout << "The line you entered: " << Line << 
        " The CalibrationValue" << CalibrationValue << std::endl;
    }

    return 0;
}
